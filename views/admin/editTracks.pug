if (jam.tracks && jam.tracks.length > 0)
  ul.list-group
  for track in jam.tracks
    li.list-group-item
      if (track.title === "--------------------")
        | &nbsp;
      else
        a(href=track.path) #{track.title}
        if (track.notes)
          | &nbsp;- #{track.notes}
        p.float-right
          a(href='javascript:playImmediately("' + track.title + '", "' + track.path + '");')
            i.fa.fa-play.fa-sm
          | &nbsp;&nbsp;&nbsp;
          a(href='javascript:deleteTrack(' + track.id + ');')
            i.far.fa-trash-alt
else
  em No tracks.
.row.mt-3
  .col
    h6 Add Audio
    .row
      .col
        #theZone.border.p-2 Drag and Drop tracks here or click to browse...
